{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/predict",
            "destination": "/api/index.py"
        },
        {
            "source": "/visitor-count",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 10
        }
    }
}